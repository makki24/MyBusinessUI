{
  "name": "mybusinessui",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "expo": "~48.0.15",
    "expo-auth-session": "~4.0.3",
    "expo-crypto": "~12.2.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "firebase": "^9.21.0",
    "react": "18.2.0",
    "react-native": "0.71.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.2",
    "@types/react": "~18.0.14",
    "@types/react-native": "^0.71.6",
    "jest": "^29.5.0",
    "typescript": "^4.9.4"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@?react-native|@?expo|@?react-navigation)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|ttf|woff|woff2|mp4|webm|svg)$": "<rootDir>/jest-mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/jest-mocks/styleMock.js"
    }
  }
}
