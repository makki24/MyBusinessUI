{
  "expo": {
    "name": "MyBusinessUI",
    "slug": "MyBusinessUI",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.syedsons.mybusiness.dev",
      "googleServicesFile": "./assets/google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.syedsons.mybusiness",
      "googleServicesFile": "./assets/GoogleService-Info.plist"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "./plugins/withNetworkStatePermission.js",
      "./plugins/withNetworkSecurityConfig.js",
      "./plugins/withCustomConfig.js",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true
          },
          "ios": {
            "useLegacyPackaging": true
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/app_production.png"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": [
      "mybusinessui",
      "com.syedsons.mybusiness",
      "com.syedsons.mybusiness.dev",
      "com.syedsons.mybusiness.qa"
    ],
    "extra": {
      "eas": {
        "projectId": "18005f4e-2007-4392-8512-60da76e2c661"
      }
    }
  }
}