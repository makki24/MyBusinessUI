    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "~3.22.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.2",
    "@types/react": "~18.2.14",
    "jest": "^29.2.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@?react-native|@?expo|@?react-navigation)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|ttf|woff|woff2|mp4|webm|svg)$": "<rootDir>/jest-mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/jest-mocks/styleMock.js"
    }
  }
