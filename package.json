{
  "name": "mybusinessui",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=local expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "prettier-format": "prettier --config .prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/analytics": "^18.5.0",
    "@react-native-firebase/app": "^18.5.0",
    "@react-native-firebase/crashlytics": "^18.5.0",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "1.6.0",
    "expo": "^51.0.22",
    "expo-auth-session": "~5.5.2",
    "expo-build-properties": "~0.12.3",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.5",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-device": "~6.0.2",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.12",
    "expo-random": "~14.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.21",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-gifted-charts": "^1.4.20",
    "react-native-paper": "^5.12.4",
    "react-native-paper-dates": "^0.22.19",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "jest": "^29.2.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "moduleNameMapper": {
      "react-dom": "react-native"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/android/**"
    ]
  },
  "overrides": {
    "semver": "^7.5.3"
  }
}
